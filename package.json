    "db:seed:all": "sequelize db:seed:all",
    "db:seed:undo:all": "sequelize db:seed:undo:all",
    "db:init": "npm run db:create && npm run db:migrate && npm run db:seed:all",
    "db:redo": "npm run db:drop && npm run db:init"
